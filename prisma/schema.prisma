// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Order {
  id        String @id @default(cuid())
  vopsit  String
  folosire  String
  forma String
  calatorit String
  nume  String
  email     String
  telefon String 
  cantitate String?
  createdAt DateTime @default(now())
}
